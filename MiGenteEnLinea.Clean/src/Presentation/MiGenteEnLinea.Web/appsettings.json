{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiConfiguration": {
    "BaseUrl": "http://localhost:5015/api",
    "StaticFilesBaseUrl": "http://localhost:5015",
    "TimeoutSeconds": 30,
    "RetryAttempts": 3,
    "HealthCheckEndpoint": "/health",
    "EnableRequestLogging": false,
    "HealthCheckTimeoutMs": 5000,
    "UserAgent": "MiGenteEnLinea.Web/1.0"
  }
}
